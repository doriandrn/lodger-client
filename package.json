{
  "name": "lodger-client",
  "version": "0.0.26",
  "description": "Administrare imobile",
  "author": "Dorian Tudorache",
  "repository": "https://github.com/doriandrn/lodger-client.git",
  "private": false,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build:app": "export ELECTRON_RUN_AS_NODE=1 && node ./electron.build.js",
    "electest": "node electron dist/index.html",
    "precommit": "npm run lint",
    "server": "pouchdb-server --host 0.0.0.0 -p 10101 -m -d /tmp/pouchdb-server/ -n true"
  },
  "main": "main.js",
  "homepage": "lodger.ro",
  "build": {
    "appId": "org.numina.lodger",
    "productName": "Lodger",
    "mac": {
      "category": "public.app-category.finance"
    },
    "dmg": {
      "icon": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Office"
    }
  },
  "dependencies": {
    "@dicebear/avatars": "^4.4.0",
