    "cashify": "^2.5.0",
    "dayjs": "^1.10.4",
    "deepmerge": "^4.2.2",
    "drag-drop": "^6.1.0",
    "easydropdown": "^4.2.0",
    "electron-serve": "^1.1.0",
    "fuzzysort": "^1.1.4",
    "human-size": "^1.1.0",
    "lodger": "^0.0.311",
    "mobx": "^5.9.4",
    "mobx-utils": "^5.6.2",
    "mobx-vue": "^2.0.11",
    "numeral": "^2.0.6",
    "nuxt": "^3.4.3",
    "object-path": "^0.11.5",
    "postcss-styl": "^0.7.2",
    "rxcollection-subscriber": "^1.0.18",
    "rxdb": "^9.15.0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-clickaway": "^2.2.2",
    "vue-tel-input": "^4.4.2",
    "vuex-toast": "^0.1.3"
  },
