{
  "name": "lodger-client",
  "version": "0.0.42",
  "description": "Administrare imobile",
  "author": "Dorian Tudorache",
  "repository": "https://github.com/doriandrn/lodger-client.git",
  "private": false,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && yarn generate",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "server": "pouchdb-server --host 0.0.0.0 -p 10101 -m -d /tmp/pouchdb-server/ -n true"
  },
  "main": "main.js",
  "homepage": "lodger.ro",
  "build": {
    "appId": "org.numina.lodger",
    "productName": "Lodger",
    "directories": {
      "buildResources": "."
    },
    "files": [
      "dist/**/*",
      "static/**/*",
      "main.js",
      "nuxt.config.js"
    ],
    "mac": {
      "category": "public.app-category.finance"
    },
    "dmg": {
      "icon": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Office"
    }
  },
  "dependencies": {
    "@dicebear/avatars": "^4.5.4",
    "@dicebear/avatars-avataaars-sprites": "^4.5.4",
